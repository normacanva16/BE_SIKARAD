{
    "version": 2,
    "builds": [
        {
            "src": "./app/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/app/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}
